{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
    },
  "env": {
    "es6": true
  },
  "rules": {
    "require-jsdoc" : 0,
    "arrow-parens": 0,
    "linebreak-style": 0,
    "no-invalid-this": 0,
    "max-statements": [2, 16],
    "max-depth": [2, 5],
    "complexity": [2, 7],
    "max-params": [2, 4],
  }
}